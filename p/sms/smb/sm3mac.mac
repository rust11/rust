.MACRO	CALL	 ROU,X1,X2,X3,X4,X5,X6,X7
	.NARG	s$mnrg
	smoff.=-cell.					
.IF NB <X1>
  .IRP	XX,<X7 X6 X5 X4 X3 X2 X1>			
