.MACRO	CLRL	DST
	.ntype ma$dst,DST
	ma$dmd=ma$dst/8.		
	ma$drg=ma$dst&7
	.if eq ma$dmd-1			
		CLR	DST		
		CLR	word.(DST)
		.mexit
	.endc
	.if eq ma$dmd-2			
		CLR	DST		
		CLR	DST
		.mexit
	.endc
	.if eq ma$dmd-4			
		CLR	DST		
		CLR	DST
		.mexit
	.endc
	.if eq ma$dmd-6			
		CLR	DST
		CLR	word.+DST	
		.mexit
	.endc
	smerr$	<Invalid mode for CLRW "DST">
.ENDM
.MACRO	MOVL	SRC,DST
	.ntype ma$src,SRC
	ma$smd=ma$src/8.		
	ma$srg=ma$src&7
	.ntype ma$dst,DST
	ma$dmd=ma$dst/8.		
	ma$drg=ma$dst&7
	ASSUME	ma$smd EQ ma$dmd	
	.if eq ma$smd-1			
		MOV	SRC,DST		
		MOV	word.'SRC,word.'DST 	
		.mexit
	.endc
