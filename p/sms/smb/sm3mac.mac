	.endc					

	.if	ge s$mmod-^x0a			
	 .if	le s$mmod-^x0e			
	  .if	eq s$mmod&1			
		pushal	OFF+ADR			
		.mexit				
	  .endc					
	 .endc					
	.endc					
	.if	eq s$mmod&6			
	 .if	ne OFF				
		pushal	OFF'ADR			
