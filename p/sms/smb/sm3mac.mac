	JSR	PC,ROU					
	.IIF	EQ s$mnrg-2,	TST	(SP)+		
	.IIF	EQ s$mnrg-3,	CMP	(SP)+,(SP)+
	.IIF	GE s$mnrg-4,	ADD	#s$mnrg-1*cell.,SP
