  (vxREA | vxCSW_|vxDAT_),
  (vxWRI | vxCSW_|vxDAT_|vxSDA_),
  (vxCLO | vxCSW_|vxSPU_),
  (vxDEL | vxCSW_|vxFNA_|vxSPU_),
  (vxLOO | vxCSW_|vxFNA_|vxSPU_|vxDIR_),
