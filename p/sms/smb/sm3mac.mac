.MACRO	SMPSH$	VAL OFF
	.ntype	s$mmod,VAL		
	.if	ne s$mmod&7-6		
	 .if idn #0,VAL			
		clr	-(sp)		
		.mexit			
	 .endc				
		mov	VAL,-(SP)	
		.mexit			
	.endc				
	.if	eq s$mmod&^o70-^o60	
		mov	OFF+VAL,-(sp)	
		.mexit			
	.endc				
	.if	eq s$mmod&^o70-^o10	
	  .if ne OFF			
		mov	OFF'VAL,-(sp)	
	  .iff				
		mov	VAL,-(sp)	
	  .endc				
		.mexit			
	.endc				
	smerr$	<"VAL" has invalid argument mode>
.ENDM

.ENDM
.MACRO	SMVPV$

.MACRO	SMPSH$	VAL OFF
	.if	idn <%extract(0,1,VAL)> <&>	
		SMPAD$	<%extract(1,60,VAL)> <OFF>
		.mexit				
	.endc					
	.ntype	s$mmod,VAL			
						
	.if	ne s$mmod&^x0f-^x0e		
	  .if	idn #0,VAL			
		clrl	-(sp)			
		.mexit				
	  .endc					
		pushl	VAL			
		.mexit				
	.endc					
	s$mmod=s$mmod@-4&^xF			
						
	.if	eq s$mmod-6			
		pushl	OFF(sp)			
		.mexit				
	.endc					

	.if	ge s$mmod-^x0a			
	 .if	le s$mmod-^x0e			
	  .if	eq s$mmod&1			
		pushl	OFF+VAL			
		.mexit				
	  .endc					
	 .endc					
	.endc					
	.if	eq s$mmod&6			
	 .if	ne OFF				
		pushl	OFF'VAL			
	  .iff					
		pushl	VAL			
	  .endc					
		.mexit				
	.endc					
	smerr$	<"VAL" has invalid argument mode>
.ENDM

.ENDM
.MACRO	SMVPA$

.MACRO	SMPAD$	ADR OFF
	.ntype	s$mmod,ADR			
	.if	ne s$mmod&^x0f-^x0e		
	  .if	idn 0,ADR			
		clrl	-(sp)			
		.mexit				
