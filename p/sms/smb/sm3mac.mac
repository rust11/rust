.MACRO	SERIES	SNAM TYP=1
.if idn TYP,bits
	.macro	..	nam val cnt=1 pos msk
		.if	nb <pos>
			m$asym=POS			
			.irp m$atmp,<\m$asym>
				$b=bit'm$atmp'$
			.endr
		.endc
		.iif	nb <msk>,$b=msk
		.if	nb <val>
			m$asym=VAL
			.irp m$atmp,<\m$asym>
			.if ne bit'm$atmp'$-$b
				smerr$	<Series missmatch "VAL">
			.endc
			.endr
		.endc
		.iif nb <nam>,nam=$b
		.rept	cnt
			$b=$b*2
		.endr
	.endm
	$b=1
.iff
	.macro	..	nam val cnt=TYP off
		.iif	nb <off>,$v=off
		.if	nb <val>
			.if ne val-$v
				smerr$	<Bit series missmatch "VAL">
			.endc
		.endc
		.iif nb <nam>,nam=$v
		$v=$v+cnt
	.endm
	$v=0
.endc
.ENDM
