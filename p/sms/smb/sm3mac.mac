		smoff.=smoff.+cell.			
		smpsh$	<XX>	\smoff.			
	.ENDC						
  .ENDR
.ENDC
	psh	#s$mnrg-1				
	mov	sp,r5					
	JSR	PC,ROU					
	.IIF	EQ s$mnrg-2,	TST	(SP)+		
	.IIF	EQ s$mnrg-3,	CMP	(SP)+,(SP)+
